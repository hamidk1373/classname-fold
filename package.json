{
	"name": "classname-fold",
	"publisher": "hamidkarami",
	"displayName": "classname-fold",
	"description": "Fold ClassName objects",
	"version": "0.1.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/hamidk1373/classname-fold"
	  },
	"engines": {
		"vscode": "^1.58.0"
	},
	"categories": [
		"Other"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "classname-fold.foldAllClassNameTag",
				"title": "Fold all classNames"
			},
			{
				"command": "classname-fold.unfoldAllClassNameTag",
				"title": "Unfold all classNames"
			}
		],
		"keybindings": [
            {
                "command": "classname-fold.foldAllClassNameTag",
                "key": "alt+]",
                "mac": "ctrl+]"
            },
			{
                "command": "classname-fold.unfoldAllClassNameTag",
                "key": "alt+[",
                "mac": "ctrl+["
            }
        ]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.58.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	}
}
